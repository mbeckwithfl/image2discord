{
    "name": "Image2Discord",
    "description": "Send images to discord",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker":"background.js",
    "browser_action": {
        "default_title": "Image2Discord", 
        "default_popup": "popup.html"  
        }
    },

    "permissions": [
        "contextMenus",
        "storage",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
      }
    
    }